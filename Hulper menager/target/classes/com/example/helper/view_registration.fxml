<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane fx:id="main1" prefHeight="314.0" prefWidth="300.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.helper.controller_registration">
    <children>
        <Button fx:id="registration_button" layoutX="83.0" layoutY="231.0" mnemonicParsing="false" prefHeight="36.0" prefWidth="133.0" style="-fx-background-color: #000000;" text="Зарегистрироваться" textFill="WHITE" />
        <Hyperlink fx:id="entry_button" layoutX="56.0" layoutY="201.0" style="-fx-color: #000000;" text="Уже есть учетная запись? Войти!" textFill="#000000e2" />
      <Text fill="#40ccd8" layoutX="48.0" layoutY="121.0" strokeType="OUTSIDE" strokeWidth="0.0">
         <font>
            <Font size="19.0" />
         </font>
      </Text>
      <TextField fx:id="login_field" layoutX="47.0" layoutY="113.0" prefHeight="27.0" prefWidth="203.0" promptText="логин" style="-fx-background-color: transpanent; -fx-border-color: #000000; -fx-border-width: 0px 0px 2px 0px;" />
      <TextField fx:id="password_field" layoutX="48.0" layoutY="140.0" prefHeight="27.0" prefWidth="203.0" promptText="пароль" style="-fx-background-color: transpanent; -fx-border-color: #000000; -fx-border-width: 0px 0px 2px 0px;" />
      <TextField fx:id="phone_field" layoutX="47.0" layoutY="167.0" prefHeight="27.0" prefWidth="203.0" promptText="телефон" style="-fx-background-color: transpanent; -fx-border-color: #000000; -fx-border-width: 0px 0px 2px 0px;" />
      <ImageView fitHeight="75.0" fitWidth="119.0" layoutX="110.0" layoutY="25.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../../../IMAGE/logo.png" />
         </image>
      </ImageView>
    </children>
</AnchorPane>
